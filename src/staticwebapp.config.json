{
    "routes": [
      {
        "route": "/*",
        "rewrite": "/index.html"
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/static/*"]
    },
    "mimeTypes": {
      ".json": "application/json",
      ".png": "image/png",
      ".jpg": "image/jpeg",
      ".jpeg": "image/jpeg",
      ".gif": "image/gif",
      ".html": "text/html",
      ".js": "application/javascript",
      ".css": "text/css"
    }
  }
  